[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Inconsolata 9 
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = on

# "block", "underline" or "ibeam"
cursor_shape = underline

# $BROWSER is used by default if set, with xdg-open as a fallback
#browser = xdg-open

# set size hints for the window
#size_hints = false

# Hide links that are no longer valid in url select overlay mode
filter_unmatched_urls = true

# emit escape sequences for extra modified keys
#modify_other_keys = false

[colors]

# special
foreground      = #dcdccc
foreground_bold = #ffffff
cursor          = #ffffff
background      = #3f3f3f

# black
color0  = #032c36
color8  = #065f73

# red
color1  = #c2454e
color9  = #ef5847

# green
color2  = #7cbf9e
color10 = #a2d9b1

# yellow
color3  = #8a7a63
color11 = #beb090

# blue
color4  = #2e3340
color12 = #61778d

# magenta
color5  = #ff5879
color13 = #ff99a1

# cyan
color6  = #44b5b1
color14 = #9ed9d8

# white
color7  = #f2f1b9
color15 = #f6f6c9

# if unset, will reverse foreground and background
highlight = #2f2f2f

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# vim: ft=dosini cms=#%s
