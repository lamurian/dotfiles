#!/bin/bash
# Script to set-up tmux layout

SES="def"

tmux has-session -t $SES &> /dev/null

if [ $? == 0 ]; then

	# man
	tmux send-key -t $SES:0.0 "ncmpcpp" ENTER
	tmux send-key -t $SES:0.1 "atop" ENTER
	tmux send-key -t $SES:0.4 "ranger $ONDR" ENTER

	# net
	tmux send-key -t $SES:1.0 "w3m duck.com"
	tmux select-pane -t $SES:1.0
	
	# Move to man
	tmux select-window -t $SES:0
	tmux select-pane -t 2

else
				
	echo "Please make session first"

fi
